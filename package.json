{
  "name": "@mopex/react-gantt-flow",
  "repository": {
    "type": "git",
    "url": "https://github.com/moppiiiii/react-gantt-flow"
  },
  "version": "0.0.1-alpha.5",
  "license": "MIT",
  "packageManager": "pnpm@10.6.3",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": ["dist"],
  "exports": {
    ".": {
      "import": "./dist/react-gantt-flow.js",
      "require": "./dist/react-gantt-flow.cjs"
    }
  },
  "module": "./dist/react-gantt-flow.js",
  "main": "./dist/react-gantt-flow.cjs",
  "scripts": {
    "prepare": "lefthook install",
    "dev": "vite",
    "dev:css": "tcm src --watch",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "chromatic": "chromatic --only-changed",
    "format:check": "biome format ./src",
    "format:fix": "biome format --write ./src",
    "lint:check": "biome lint ./src",
    "lint:fix": "biome lint --write ./src",
    "knip": "knip --config knip.json",
    "test:unit": "vitest run"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  },
  "peerDependencies": {
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@chromatic-com/storybook": "^3.2.4",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@eslint/js": "^9.17.0",
    "@storybook/addon-essentials": "^8.5.1",
    "@storybook/addon-interactions": "^8.5.1",
    "@storybook/addon-onboarding": "^8.5.1",
    "@storybook/react": "^8.5.1",
    "@storybook/react-vite": "^8.5.1",
    "@storybook/test": "^8.5.1",
    "@types/node": "^22.10.9",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "chromatic": "^11.25.1",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-storybook": "^0.12.0",
    "globals": "^15.14.0",
    "knip": "^5.43.1",
    "lefthook": "^1.10.10",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "storybook": "^8.5.1",
    "typed-css-modules": "^0.9.1",
    "typescript": "~5.8.0",
    "typescript-eslint": "^8.18.2",
    "vite": "^6.0.5",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-lib-inject-css": "^2.2.2",
    "vitest": "^3.0.4"
  },
  "eslintConfig": {
    "extends": ["plugin:storybook/recommended"]
  },
  "pnpm": {
    "overrides": {
      "vite@>=6.2.0 <6.2.3": ">=6.2.3",
      "vite@>=6.2.0 <6.2.4": ">=6.2.4",
      "vite@>=6.2.0 <6.2.6": ">=6.2.6",
      "vite@>=6.2.0 <=6.2.6": ">=6.2.7",
      "vite@>=6.2.0 <6.2.5": ">=6.2.5"
    }
  }
}
